<jni4net-proxygen xmlns="http://jni4net.sf.net/0.8.6.0/toolConfig.xsd">
  <!-- Location where .java files will be generated into. It's relative or absolute path. -->
  <TargetDirJvm>jvm</TargetDirJvm>

  <!-- Location where .cs files will be generated into. It's relative or absolute path. -->
  <!-- This is the default location in which the generated batch files look for cs sources -->
  <TargetDirClr>clr</TargetDirClr>

  <!-- List of class paths (.jar files or directories) which define known classes for generator-->
  <ClassPath Path="lib/closure/compiler.jar"/>

  <!-- List of Java classes which should have proxy generated -->
  <JavaClass TypeName="com.google.javascript.jscomp.JSError"/>
  <JavaClass TypeName="com.google.javascript.jscomp.Result"/>
  <JavaClass TypeName="com.google.javascript.jscomp.JSSourceFile"/>
  <JavaClass TypeName="com.google.javascript.jscomp.SourceFile"/>
  <JavaClass TypeName="com.google.javascript.jscomp.Compiler"/>
  <JavaClass TypeName="com.google.javascript.jscomp.CompilerOptions"/>
  <JavaClass TypeName="com.google.javascript.jscomp.CompilationLevel"/>
  <JavaClass TypeName="com.google.javascript.jscomp.WarningLevel"/>
  <JavaClass TypeName="com.google.javascript.jscomp.CheckLevel"/>
  <JavaClass TypeName="com.google.javascript.jscomp.DiagnosticGroups"/>
  <JavaClass TypeName="com.google.javascript.jscomp.CommandLineRunner"/>

</jni4net-proxygen>
